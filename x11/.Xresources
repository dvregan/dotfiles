URxvt*font: xft:Fira Code:size=9:antialias=true
URxvt*letterSpace: -1
URxvt*borderLess: false
URxvt*externalBorder: 0
URxvt*internalBorder: 16
URxvt*scrollBar: false
URxvt*saveLines: 5000

URxvt*depth: 32
URxvt*background: rgba:0000/0000/0200/c800

URxvt*perl-ext-common: default,tabbedex

URxvt.keysym.A-S-N: perl:tabbedex:new_tab
URxvt.keysym.A-S-H: perl:tabbedex:move_tab_left
URxvt.keysym.A-S-J: perl:tabbedex:prev_tab
URxvt.keysym.A-S-K: perl:tabbedex:next_tab
URxvt.keysym.A-S-L: perl:tabbedex:move_tab_right
URxvt.keysym.A-S-R: perl:tabbedex:rename_tab

URxvt*tabbedex.tabbar-fg: 15
URxvt*tabbedex.tabbar-bg: 236
URxvt*tabbedex.tab-fg: 110
URxvt*tabbedex.tab-bg: 236

URxvt*tabbedex.transparent: true
URxvt*tabbedex.new-button: false
