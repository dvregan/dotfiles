*.foreground: #c6c6c6
*.background: #1c1c1c
*.cursorColor: #444

! black 236 238
*.color0: #333
*.color8: #444

! red 167
*.color1: #d75f5f
*.color9: #d75f5f

! green 108
*.color2: #87af87
*.color10: #87af87

! yellow 221
*.color3: #ffd75f
*.color11: #ffd75f

! blue 110
*.color4: #87afd7
*.color12: #87af87

! magenta 146
